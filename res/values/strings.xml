    <!-- Help -->
    <string name="help_add_sensor_content">Press ADD to open the sensor configuration screen. To save the sensor\'s configuration, press SAVE.</string>
    <string name="help_add_sensor_title">Add Sensor</string>
    <string name="help_analog_input_content">Use ANALOG INPUT to measure the voltage levels from 0V to 3.3V (default) or scaled to a range from 0 to 1. Get detailed information under\nhttp://github.com/ytai/ioio/wiki/Analog-Input</string>
    <string name="help_analog_input_title">Analog Input</string>
    <string name="help_delete_measurements_content">Press EDIT to activate ActionMode. To delete a specific value, click on the value\'s REMOVE-icon. To delete all values, press DELETE ALL.</string>
    <string name="help_delete_measurements_title">Delete Measured Values</string>
    <string name="help_delete_sensor_content">Press EDIT to activate ActionMode. To delete a specific sensor, click on the sensor\'s REMOVE-icon. To delete all sensors, press DELETE ALL.</string>
    <string name="help_delete_sensor_title">Delete Sensor</string>
    <string name="help_digital_input_content">Use DIGITAL INPUT to measure LOW (0 = false) or HIGH (1 = true). Get detailed information under\nhttp://github.com/ytai/ioio/wiki/Digital-IO</string>
    <string name="help_digital_input_title">Digital Input</string>
    <string name="help_edit_sensor_content">Do a long-click on the sensor to edit or see a its configuration. To save changes press SAVE.</string>
    <string name="help_edit_sensor_title">Edit Sensor</string>
    <string name="help_pause_sensor_content">Click on the sensor\'s PAUSE/PLAY-icon to pause or unpause the sensor. Only unpaused sensors are used during measurement processes.</string>
    <string name="help_pause_sensor_title">Pause Sensor</string>
    <string name="help_export_measurements_content">Press EXPORT to export the measured values table as a .csv-file to the external storage.</string>
    <string name="help_export_measurements_title">Export Measured Values</string>
    <string name="help_export_sensors_content">Press EXPORT to export the sensor configuration table as a .csv-file to the external storage.</string>
    <string name="help_export_sensors_title">Export Sensor Configuration</string>
    <string name="help_refresh_measurements_content">Press REFRESH to refresh the measured values list. Can be used during measurement processes.</string>
    <string name="help_refresh_measurements_title">Refresh List</string>
    <string name="help_show_measurements_content">Click on a sensor to open the measurements screen.</string>
    <string name="help_show_measurements_title">Show Measured Values</string>
    <string name="help_threshold_content">You can enter a threshold value. If the threshold type is set to LOWER, only measured values >= the threshold value will be saved in the database (&lt;= for threshold type UPPER). </string>
    <string name="help_threshold_title">Threshold</string>
    <string name="help_upload_measurements_content">Press UPLOAD to upload measured values to Xively. To use this feature you must enter your API-key and Feed ID in the settings, enable the sensor to use Xively and enter the according datastream.</string>
    <string name="help_upload_measurements_title">Upload Measured Values</string>
    <string name="help_xively_content">If activated you can upload measured values to your feed on Xively. For that enter the datastream from Xively that represents the sensor.</string>
    <string name="help_xively_title">Xively</string>

